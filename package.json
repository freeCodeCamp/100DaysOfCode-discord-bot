{
  "name": "100-days-of-code-bot",
  "version": "1.2.0",
  "description": "A bot for tracking 100 Days of Code progress in Discord.",
  "main": "./prod/index.js",
  "scripts": {
    "prebuild": "rm -rf ./prod",
    "build": "tsc",
    "lint": "eslint ./src",
    "start": "node -r dotenv/config ./prod/index.js",
    "test": "echo 'No tests yet'.",
    "docs": "docsify serve ./docs -o --port 3200"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nhcarrigan/100-days-of-code-bot.git"
  },
  "author": "Nicholas Carrigan",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/nhcarrigan/100-days-of-code-bot/issues"
  },
  "homepage": "https://github.com/nhcarrigan/100-days-of-code-bot#readme",
  "engines": {
    "node": "16.2.0",
    "npm": "7.20.6"
  },
  "devDependencies": {
    "@types/node": "15.12.1",
    "@typescript-eslint/eslint-plugin": "4.29.1",
    "@typescript-eslint/parser": "4.29.1",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "husky": "7.0.1",
    "lint-staged": "11.1.2",
    "prettier": "2.3.2"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@sentry/integrations": "6.11.0",
    "@sentry/node": "6.11.0",
    "discord.js": "13.0.0",
    "docsify-cli": "4.4.3",
    "dotenv": "10.0.0",
    "mongoose": "5.13.7",
    "winston": "3.3.3"
